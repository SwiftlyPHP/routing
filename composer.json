{
    "name": "swiftly/routing",
    "description": "A very simple regex router.",
    "homepage": "https://github.com/SwiftlyPHP/routing",
    "readme": "README.md",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8",
        "ext-json": "*"
    },
    "require-dev": {
        "vimeo/psalm": "^5.15",
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
            "Swiftly\\Routing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swiftly\\Routing\\Tests\\": "tests/"
        }
    }
}
